{
  "name": "learnwords",
  "version": "1.0.0",
  "private": true,
  "author": "Anatol Marezhanyi aka e1r0nd//[CRG]",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0",
    "npm": "^3.0.0"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "http-server app/ -a 0.0.0.0 -p 8000 -o",
    "build:dev": "set NODE_ENV=development&webpack",
    "build:prod": "set NODE_ENV=production&webpack",
    "test": "mocha --compilers js:babel-core/register",
    "coverage": "istanbul cover --include-all-sources node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register -R spec test/",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "webpack": "set NODE_ENV=testing&webpack --profile --display-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e1r0nd/learnwords.git"
  },
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "bootstrap": "^3.3.7",
    "coveralls": "^2.11.15",
    "html-loader": "^0.4.4",
    "http-server": "^0.9.0",
    "istanbul": "^1.1.0-alpha.1",
    "jquery": "^3.1.1",
    "materialize-css": "^0.97.8",
    "mocha": "^3.2.0",
    "npm": "^4.1.2",
    "webpack": "^1.14.0"
  },
  "devDependencies": {}
}
